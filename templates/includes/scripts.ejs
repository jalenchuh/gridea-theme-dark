<% if (site.customConfig.preload == 'quicklink') { %>
  <script src="https://cdn.jsdelivr.net/npm/quicklink@2.0.0/dist/quicklink.umd.js"></script>
  <script>quicklink.listen();</script>
<% } %>

<% if (site.customConfig.preload == 'instant.page') { %>
  <script src="https://cdn.jsdelivr.net/gh/instantpage/instant.page@5.1.0/instantpage.js"></script>
<% } %>

<% if (site.customConfig.preload == 'FlyingPages') { %>
  <script>
    window.FPConfig = {
      delay: 0,
      ignoreKeywords: ["#"],
      maxRPS: 5,
      hoverDelay: 50
    };
  </script>
  <script defer src="https://cdn.jsdelivr.net/gh/gijo-varghese/flying-pages@2.1.2/flying-pages.min.js"></script>
<% } %>
