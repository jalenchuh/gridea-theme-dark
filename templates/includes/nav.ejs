<div class="hd">
  <div class="logodiv">
    <img class="logopng" src="<%= themeConfig.domain %>/images/avatar.png">
    <p class="logot">
      <span class="logo_1">&lt;</span><span class="logo_2">/</span><span class="logo_3">#</span> <%= site.customConfig.author %> <span class="logo_3">#</span><span class="logo_2">/</span><span class="logo_1">&gt;</span>
    </p>
  </div>
  <ol class="breadcrumb">
    <% menus.forEach(function(menu) { %>
      <% if (menu.openType === 'External') { %>
        <li>
          <a class="ba" href="<%= menu.link %>"
             target="_blank" rel="nofollow noopener">
             <%= menu.name %></a
          >
        </li>
      <% } else { %>
        <li>
          <a class="ba" href="<%= menu.link %>">
            <%= menu.name %>
          </a>
        </li>
      <% } %>
    <% }); %>
  </ol>
  <hr class="breadcrumbhr">
</div>
